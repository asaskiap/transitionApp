{
  "expo": {
    "name": "transitionApp",
    "slug": "transitionApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appLogo.png",
    "splash": {
      "image": "./assets/AppSplash_theFall.png",
      "resizeMode": "cover",
      "backgroundColor": "#121e2a"
    },
    "updates": {
      "fallbackToCacheTimeout": 5000
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/AppLogoTransparent.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
